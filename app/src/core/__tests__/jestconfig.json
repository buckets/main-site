{
  "rootDir": "../../..",
  "globals": {
    "__TS_CONFIG__": "./src/core/tsconfig.json"
  },
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ],
  "transform": {
    "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "testRegex": "src/core/__tests__/test_.*\\.(ts|tsx|js)$"
}