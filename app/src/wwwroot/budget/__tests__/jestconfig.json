{
  "rootDir": "../../../..",
  "globals": {
    "__TS_CONFIG__": {
      "compilerOptions": {
        "module": "commonjs"
      }
    }
  },
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json"
  ],
  "transform": {
    "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "testRegex": "src/wwwroot/budget/__tests__/.*test_.*\\.(ts|tsx|js)$"
}