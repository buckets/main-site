{
  "name": "Buckets",
  "version": "0.50.0-dev",
  "homepage": "https://www.budgetwithbuckets.com",
  "main": "src/mainprocess/main.js",
  "description": "Buckets Family Budgeting",
  "private": true,
  "author": {
    "name": "Buckets",
    "email": "hello@budgetwithbuckets.com"
  },
  "scripts": {
    "docs": "typedoc --tsconfig tsconfig.json --theme minimal --exclude '**/*.{test,spec}.*' --out devdocs/ src/",
    "start": "electron .",
    "clean": "find src -name '*.js' -exec rm {} \\;",
    "compile": "cross-env NODE_ENV=production tsc",
    "test": "./test.sh && nsp check",
    "postinstall": "electron-builder install-app-deps",
    "fakebank": "http-server -a localhost -p 8080 test/fakebank"
  },
  "dependencies": {
    "@iffycan/i18n": "^0.7.0",
    "bluebird": "3.5.0",
    "buckets-core": "file:../core",
    "chardet": "^0.5.0",
    "classnames": "2.2.5",
    "color": "^3.0.0",
    "csv": "^2.0.0",
    "d3": "^4.10.0",
    "electron-is": "^2.4.0",
    "electron-log": "^2.2.14",
    "electron-updater": "^2.21.4",
    "fs-extra": "^4.0.0",
    "fs-extra-promise": "^1.0.1",
    "iconv-lite": "^0.4.23",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "lodash.sortby": "^4.7.0",
    "moment-timezone": "^0.5.21",
    "ofx-js": "^0.1.1",
    "prop-types": "^15.6.1",
    "qs": "6.4.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-transition-group": "^2.3.0",
    "request": "^2.81.0",
    "request-promise": "4.2",
    "sqlite3-offline": "^3.2.0",
    "tmp": "0.0.31",
    "triplesec": "3.0.26",
    "uuid": "^3.0.1"
  },
  "build": {
    "appId": "com.github.buckets.application",
    "mac": {
      "category": "public.app-category.finance",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "dmg": {
      "icon": "build/dmg.icns"
    },
    "nsis": {
      "perMachine": true
    },
    "linux": {
      "category": "Finance",
      "target": [
        "tar.gz",
        "deb",
        "AppImage"
      ]
    },
    "fileAssociations": [
      {
        "ext": "buckets",
        "role": "Editor",
        "icon": "build/doc.icns"
      }
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "buckets",
        "repo": "application"
      }
    ],
    "files": [
      "!**/*.ts",
      "!**/*.tsx",
      "!*.sh",
      "!.dockerignore",
      "!.taprc",
      "!.nyc_output",
      "!buckets.sublime-*",
      "!coverage${/*}",
      "!dev${/*}",
      "!devdocs${/*}",
      "!mocha.opts",
      "!nodesrc${/*}",
      "!README.md",
      "!test${/*}",
      "!TODO",
      "!tsconfig.json",
      "!watchtest.js"
    ]
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.5",
    "@types/chai": "^4.0.0",
    "@types/chardet": "^0.5.0",
    "@types/cheerio": "^0.22.2",
    "@types/color": "^3.0.0",
    "@types/d3": "^4.10.0",
    "@types/fs-extra": "^4.0.0",
    "@types/fs-extra-promise": "^1.0.2",
    "@types/jsonwebtoken": "^7.2.2",
    "@types/keytar": "^4.0.1",
    "@types/lodash": "^4.14.106",
    "@types/mocha": "^2.2.41",
    "@types/moment-timezone": "^0.5.6",
    "@types/node": "^9.6.1",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.3.1",
    "@types/react-dom": "^16.0.4",
    "@types/react-transition-group": "^2.0.7",
    "@types/request": "^2.47.0",
    "@types/request-promise": "^4.1.41",
    "@types/uuid": "^3.0.0",
    "asar": "^0.14.3",
    "chai": "4.0.2",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.1.3",
    "electron": "1.7.9",
    "electron-builder": "^20.13.3",
    "http-server": "^0.10.0",
    "mocha": "3.4.2",
    "nsp": "^3.2.1",
    "tap": "^10.7.0",
    "ts-node": "3.0.6",
    "typedoc": "^0.9.0",
    "typescript": "^2.8.1",
    "watch": "^1.0.2"
  }
}
