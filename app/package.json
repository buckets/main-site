{
  "name": "Buckets",
  "version": "0.41.0-dev",
  "homepage": "https://www.budgetwithbuckets.com",
  "main": "src/mainprocess/main.js",
  "description": "Buckets Family Budgeting",
  "author": {
    "name": "Buckets",
    "email": "hello@budgetwithbuckets.com"
  },
  "scripts": {
    "docs": "typedoc --tsconfig tsconfig.json --theme minimal --exclude '**/*.{test,spec}.*' --out devdocs/ src/",
    "start": "electron .",
    "compile": "cross-env NODE_ENV=production tsc",
    "test": "./test.sh",
    "postinstall": "install-app-deps",
    "fakebank": "http-server -a localhost -p 8080 test/fakebank"
  },
  "dependencies": {
    "bluebird": "3.5.0",
    "classnames": "2.2.5",
    "color": "^3.0.0",
    "csv": "^2.0.0",
    "d3": "^4.10.0",
    "electron-is": "^2.4.0",
    "electron-log": "^2.2.7",
    "electron-updater": "2.1.2",
    "fs-extra": "^4.0.0",
    "fs-extra-promise": "^1.0.1",
    "jsonwebtoken": "7.4.1",
    "keytar": "^4.0.4",
    "lodash": "4.17.4",
    "mathjs": "3.13.3",
    "moment": "2.18.1",
    "moment-timezone": "^0.5.14",
    "ofx-js": "^0.1.0",
    "prop-types": "15.5.10",
    "qs": "6.4.0",
    "react": "15.6.1",
    "react-addons-css-transition-group": "15.6.0",
    "react-dom": "15.6.1",
    "request": "^2.81.0",
    "request-promise": "4.2.1",
    "sqlite": "^2.9.1",
    "tmp": "0.0.31",
    "triplesec": "^3.0.26",
    "uuid": "^3.0.1"
  },
  "build": {
    "appId": "com.github.buckets.application",
    "mac": {
      "category": "public.app-category.finance",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "dmg": {
      "icon": "build/dmg.icns"
    },
    "nsis": {
      "perMachine": true
    },
    "linux": {
      "category": "Finance",
      "target": [
        "tar.gz",
        "deb",
        "AppImage"
      ]
    },
    "fileAssociations": [
      {
        "ext": "buckets",
        "role": "Editor",
        "icon": "build/doc.icns"
      }
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "buckets",
        "repo": "application"
      }
    ],
    "files": [
      "!**/*.ts",
      "!**/*.tsx",
      "!*.sh",
      "!.dockerignore",
      "!.taprc",
      "!.nyc_output",
      "!buckets.sublime-*",
      "!coverage${/*}",
      "!dev${/*}",
      "!devdocs${/*}",
      "!mocha.opts",
      "!nodesrc${/*}",
      "!README.md",
      "!test${/*}",
      "!TODO",
      "!tsconfig.json",
      "!watchtest.js"
    ]
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.5",
    "@types/chai": "^4.0.0",
    "@types/cheerio": "^0.22.2",
    "@types/color": "^3.0.0",
    "@types/d3": "^4.10.0",
    "@types/electron-builder": "2.8.0",
    "@types/fs-extra": "^4.0.0",
    "@types/fs-extra-promise": "^1.0.2",
    "@types/jsonwebtoken": "^7.2.2",
    "@types/keytar": "^4.0.1",
    "@types/lodash": "^4.14.77",
    "@types/mathjs": "^0.0.34",
    "@types/mocha": "^2.2.41",
    "@types/moment-timezone": "^0.5.4",
    "@types/prop-types": "^15.5.1",
    "@types/react": "15.0.29",
    "@types/react-addons-css-transition-group": "^15.0.3",
    "@types/react-dom": "15.5.0",
    "@types/request": "^0.0.45",
    "@types/request-promise": "^4.1.36",
    "@types/uuid": "^3.0.0",
    "asar": "^0.13.0",
    "chai": "4.0.2",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.1.3",
    "electron": "1.7.9",
    "electron-builder": "19.43.3",
    "http-server": "^0.10.0",
    "mocha": "3.4.2",
    "tap": "^10.7.0",
    "ts-node": "3.0.6",
    "typedoc": "^0.9.0",
    "typescript": "^2.5.2",
    "watch": "^1.0.2"
  }
}
