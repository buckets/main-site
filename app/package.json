{
  "name": "Buckets",
  "version": "0.11.0",
  "homepage": "https://www.bucketsisbetter.com",
  "main": "src/mainprocess/main.js",
  "description": "Buckets Family Budgeting",
  "author": {
    "name": "Buckets",
    "email": "hello@bucketsisbetter.com"
  },
  "scripts": {
    "start": "electron .",
    "compile": "tsc",
    "test": "./test.sh",
    "postinstall": "install-app-deps"
  },
  "dependencies": {
    "@types/fs-extra": "^4.0.0",
    "@types/fs-extra-promise": "^1.0.2",
    "@types/mathjs": "0.0.34",
    "@types/prop-types": "15.5.1",
    "@types/react-addons-css-transition-group": "15.0.3",
    "@types/request-promise": "^4.1.36",
    "adm-zip": "0.4.7",
    "bluebird": "3.5.0",
    "classnames": "2.2.5",
    "electron-is": "^2.4.0",
    "electron-log": "^2.2.7",
    "electron-updater": "2.1.2",
    "fs-extra": "^4.0.0",
    "fs-extra-promise": "^1.0.1",
    "jsonwebtoken": "7.4.1",
    "lodash": "4.17.4",
    "mathjs": "3.13.3",
    "moment": "2.18.1",
    "ofx-js": "^0.1.0",
    "prop-types": "15.5.10",
    "qs": "6.4.0",
    "react": "15.6.1",
    "react-addons-css-transition-group": "15.6.0",
    "react-dom": "15.6.1",
    "request": "^2.81.0",
    "request-promise": "4.2.1",
    "sqlite": "2.8.0",
    "tmp": "0.0.31",
    "typescript": "2.3.4",
    "uuid": "^3.0.1"
  },
  "build": {
    "appId": "com.github.buckets.application",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "dmg": {
      "icon": "build/dmg.icns"
    },
    "nsis": {
      "perMachine": true
    },
    "linux": {
      "target": [
        "tar.gz",
        "deb"
      ]
    },
    "fileAssociations": [
      {
        "ext": "buckets",
        "role": "Editor",
        "icon": "build/doc.icns"
      }
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "buckets",
        "repo": "application"
      }
    ],
    "files": [
      "!.dockerignore",
      "!**/*.ts",
      "!**/*.tsx",
      "!*.sh",
      "!.taprc",
      "!buckets.sublime-*",
      "!coverage${/*}",
      "!dev${/*}",
      "!mocha.opts",
      "!README.md",
      "!TODO",
      "!tsconfig.json",
      "!watchtest.js"
    ]
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.5",
    "@types/chai": "^4.0.0",
    "@types/electron-builder": "2.8.0",
    "@types/jsonwebtoken": "^7.2.2",
    "@types/mocha": "^2.2.41",
    "@types/react": "15.0.29",
    "@types/react-dom": "15.5.0",
    "@types/request": "^0.0.45",
    "@types/uuid": "^3.0.0",
    "asar": "^0.13.0",
    "chai": "4.0.2",
    "electron": "1.6.10",
    "electron-builder": "18.6.2",
    "electron-rebuild": "1.5.11",
    "mocha": "3.4.2",
    "tap": "^10.7.0",
    "ts-node": "3.0.6",
    "watch": "^1.0.2"
  }
}
